@model IEnumerable<Sklep_Internetowy.Models.Contact>
@{
    ViewBag.Title = "Lista zgłoszeń klientów";
    Layout = "~/Views/Shared/_ManageLayout.cshtml";
}

<h2 style="text-align:center;">Lista zgłoszeń klientów</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstNameAndLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageContact)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstNameAndLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MessageContact)
            </td>
        </tr>
    }
</table>
